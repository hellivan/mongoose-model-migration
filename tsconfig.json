{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "removeComments": true,
        "sourceMap": true,
        "outDir": "dist",
        "declaration": true,
        "strict": true,
        "incremental": true,
        "importHelpers": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true
    },
    "exclude": ["node_modules", "dist", "test"]
}
